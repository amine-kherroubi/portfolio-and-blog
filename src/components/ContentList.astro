---
/**
 * ContentList Component - Desktop Only
 */

interface Props {
  class?: string;
}

const { class: className = "" } = Astro.props;
---

<section class="py-16 px-8" aria-label="Content list">
  <div class:list={["max-w-container mx-auto space-y-0", className]}>
    <slot />
  </div>
</section>
